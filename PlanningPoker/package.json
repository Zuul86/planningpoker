{
  "name": "planningpoker",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter mocha-trx-reporter --reporter-options output=myResult.trx tools/testSetup.js \"src/**/*.test.js\"",
    "test:coverage": "npm nyc --reporter=html --reporter=text test",
    "test:watch": "npm run test -- --watch",
    "testlocal": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js"
  },
  "repository": {
    "type": "git",
    "url": "https://zuul86.visualstudio.com/_git/PlanningPoker"
  },
  "author": "",
  "license": "MIT",
